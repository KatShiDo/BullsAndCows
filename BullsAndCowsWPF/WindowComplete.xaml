<Window x:Class="BullsAndCowsWPF.WindowComplete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:BullsAndCowsWPF"
        mc:Ignorable="d"
        FontSize="40"
        WindowStyle="None"
        ResizeMode="NoResize"
        Title="WindowComplete"
        Width="300"
        Height="200">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="50">
        <TextBlock Text="You Win!!!" HorizontalAlignment="Center"></TextBlock>
        <Button Content="Again!" Click="Button_Click" HorizontalAlignment="Center"
                VerticalAlignment="Center" Margin="10" FontSize="30"></Button>
    </StackPanel>
</Window>
